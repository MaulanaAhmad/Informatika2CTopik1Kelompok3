public class atributWarga {
    public String nik;
    private String nama;
    private String tglLahir;
    private String jenisKelamin;
    private String alamat;
    private String agama;
    private String pekerjaan;
    private String kewarganegaraan;
    public atributWarga(String nikWarga, String namaWarga, String tglLahirWarga, 
            String jenisKelaminWarga, String alamatWarga, String agamaWarga, 
            String pekerjaanWarga, String kewarganegaraanWarga){
        setNik(nikWarga);
        setNama(namaWarga);
        setTglLahir(tglLahirWarga);
        setJenisKelamin(jenisKelaminWarga);
        setAlamat(alamatWarga);
        setAgama(agamaWarga);
        setPekerjaan(pekerjaanWarga);
        setKewarganegaraan(kewarganegaraanWarga);
    }

    public String getNik() {
        return nik;
    }

    public void setNik(String nik) {
        this.nik = nik;
    }

    public String getNama() {
        return nama;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public String getTglLahir() {
        return tglLahir;
    }

    public void setTglLahir(String tglLahir) {
        this.tglLahir = tglLahir;
    }

    public String getJenisKelamin() {
        return jenisKelamin;
    }

    public void setJenisKelamin(String jenisKelamin) {
        this.jenisKelamin = jenisKelamin;
    }

    public String getAlamat() {
        return alamat;
    }

    public void setAlamat(String alamat) {
        this.alamat = alamat;
    }

    public String getAgama() {
        return agama;
    }

    public void setAgama(String agama) {
        this.agama = agama;
    }

    public String getPekerjaan() {
        return pekerjaan;
    }

    public void setPekerjaan(String pekerjaan) {
        this.pekerjaan = pekerjaan;
    }

    public String getKewarganegaraan() {
        return kewarganegaraan;
    }

    public void setKewarganegaraan(String kewarganegaraan) {
        this.kewarganegaraan = kewarganegaraan;
    }
}
